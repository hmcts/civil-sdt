<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping >

      <class name="uk.gov.moj.sdt.domain.BulkCustomer" table="BULK_CUSTOMERS" lazy="false" >
              
            <id name="id" column="BULK_CUSTOMER_ID" >  
                  <generator class="native" />
            </id>
            
            <property name="version" column="VERSION_NUMBER"/>
            <property name="customerCaseCode" column="BULK_CUSTOMER_CASE_CODE" />
            <property name="sdtCustomerId" column="SDT_CUSTOMER_ID"/>

      </class>

</hibernate-mapping>

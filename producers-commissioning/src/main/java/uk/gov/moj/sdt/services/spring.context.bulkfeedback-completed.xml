<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<!-- This is the spring configuration file for initialising completed bulk feed back data -->

	<!-- Submission date, this maps to created date in bulk submission -->
	<bean id="org.joda.time.LocalDateTime.submissionDateCompleted" class="org.joda.time.LocalDateTime">
		<!-- Use static date of 22 July 2013 -->
		 <constructor-arg index="0" value="2013"/>
		 <constructor-arg index="1" value="7"/>
		 <constructor-arg index="2" value="22"/>
		 <constructor-arg index="3" value="13"/>
		 <constructor-arg index="4" value="0"/>		
	</bean>
	
	<!-- Bulk Submission definition -->
	<bean id="uk.gov.moj.sdt.domain.api.IBulkSumissionCompleted" class="uk.gov.moj.sdt.domain.BulkSubmission">     
        <property name="customerReference" value="USER_FILE_REFERENCE_B2"/>
        <property name="createdDate" ref="org.joda.time.LocalDateTime.submissionDateCompleted"/>
        <property name="numberOfRequest" value="16"/>
        <property name="submissionStatus" value="Completed"/>   
    </bean>
    
    <!-- Define the Bulk Feedback Factory and pass in the Bulk Submission in the constructor -->
    <bean id="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryCompleted" class="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactory">    	
    	<constructor-arg ref="uk.gov.moj.sdt.domain.api.IBulkSumissionCompleted"/>
    	<!-- Define the system specific response -->
    	<property name="targetResponse">
    		<value>
    			<![CDATA[
				<mcolResponseDetail>
                    <claimNumber>12345678</claimNumber>
                    <issueDate>2012-11-11</issueDate>
                    <serviceDate>2012-11-11</serviceDate>
                    <warrantNumber>12345678</warrantNumber>
                    <enforcingCourtCode>123</enforcingCourtCode>
                    <enforcingCourtName>enforcing_court_name</enforcingCourtName>
                    <fee>9999</fee>
                </mcolResponseDetail>
                ]]>
			</value>
    	</property>
    </bean>    
        
    <!-- Create the individual requests via method invocation -->   	
 	<bean id="createIndividualRequestCompleted" class="org.springframework.beans.factory.config.MethodInvokingFactoryBean">
 		<property name="targetObject"><ref local="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryCompleted"/></property>   		
   		<property name="targetMethod" value="createIndividualRequest"/>
   		<property name="arguments">
     		<list>
     			<list>
	     			<!-- Unique  Request Identifier-->
	       			<value>USER_REQUEST_ID_B1</value>
	       			<!-- Request Type -->
	       			<value>mcolClaim</value>
	       			<!-- Request Status -->
	       			<value>Accepted</value>
	       			<!-- Rejection Reason Code -->
	       			<value></value>
	       			<!-- Rejection Reason Description -->
	       			<value></value>
	       			<!-- Target response -->
	       			<value>true</value>
	       		</list>
	       		<list>	     				       		       		
	       			<value>USER_REQUEST_ID_B1</value>	       			
	       			<value>mcolClaim</value>	       			
	       			<value>Rejected</value>	       			
	       			<value>DUP_CUST_REQID</value>	       			
	       			<value>Duplicate User Request Identifier submitted USER_REQUEST_ID_B1.</value>
	       			<value>false</value>
	       		</list>
	       		<list>	       					
	       			<value>USER_REQUEST_ID_B2</value>	       			
	       			<value>mcolClaim</value>	       			
	       			<value>Rejected</value>	       			
	       			<value>39</value>	       			
	       			<value>First defendant's postcode is not in England or Wales.</value>
	       			<value>true</value>
	       		</list>
	       		<list>	  
	       			<value>USER_REQUEST_ID_B3</value>	       			
	       			<value>mcolJudgment</value>	       			
	       			<value>Rejected</value>	       			
	       			<value></value>	       			
	       			<value>A bar on judgment/enforcement is in place.</value>
	       			<value>true</value>
	       		</list>
	       		<list>	       		   			
	       			<value>USER_REQUEST_ID_B4</value>	       			
	       			<value>mcolClaimStatusUpdate</value>	       			
	       			<value>Accepted</value>	       			
	       			<value></value>	       			

<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <!-- Mock implementation for Submit Query Service -->        
    <bean id="uk.gov.moj.sdt.services.api.ISubmitQueryService" 
        class="uk.gov.moj.sdt.services.MockSubmitQueryService">        
    </bean>

  
  	<!-- Mock implementation of Bulk Submission DAO -->
    <bean id="uk.gov.moj.sdt.dao.MockBulkSubmissionDao" 
        class="uk.gov.moj.sdt.dao.MockBulkSubmissionDao">
    </bean>


    <!-- Mock implementation for Bulk Feedback Service -->        
    <bean id="uk.gov.moj.sdt.services.api.IBulkFeedbackService" 
        class="uk.gov.moj.sdt.services.MockBulkFeedbackService">
        
        <property name="bulkFeedbackFactoryMap">
        	<map>
        		<!-- These keys also appear in MockBulkSubmissionDao.java for the SDT bulk reference check -->
        		<entry key="MCOL_20130722_A00000001" value-ref="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryValidated"/>
        		<entry key="MCOL_20130722_B00000001" value-ref="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryUploaded"/>
        		<entry key="MCOL_20130722_B00000002" value-ref="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryCompleted"/>        		
        		<entry key="MCOL_20130722_C00000001" value-ref="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryError"/> 
        	</map>
        </property>                
    </bean>	
</beans>
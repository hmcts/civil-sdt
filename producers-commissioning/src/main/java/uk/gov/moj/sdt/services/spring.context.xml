<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <!-- Mock implementation for Submit Query Service -->        
    <bean id="uk.gov.moj.sdt.services.api.ISubmitQueryService" 
        class="uk.gov.moj.sdt.services.MockSubmitQueryService">        
    </bean>

    <!-- Mock implementation for Bulk Feedback Service -->        
    <bean id="uk.gov.moj.sdt.services.api.IBulkFeedbackService" 
        class="uk.gov.moj.sdt.services.MockBulkFeedbackService">
        
        <property name="bulkFeedbackValidated" ref="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryValidated" />
        <property name="bulkFeedbackUploaded" ref="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryUploaded" />        
        <property name="bulkFeedbackCompleted" ref="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryCompleted" />
        <property name="bulkFeedbackError" ref="uk.gov.moj.sdt.producers.comx.utils.BulkFeedbackFactoryError" />         
                
    </bean>
	
</beans>
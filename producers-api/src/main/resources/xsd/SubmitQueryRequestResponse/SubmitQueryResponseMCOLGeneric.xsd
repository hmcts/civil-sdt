<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://ws.sdt.moj.gov.uk/2013/mcol/SubmitQueryResponseSchema" 
    targetNamespace="http://ws.sdt.moj.gov.uk/2013/mcol/SubmitQueryResponseSchema"
    xmlns:base="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" 
	elementFormDefault="qualified"
    version="1.0">
    
	<xs:import namespace="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" schemaLocation="../RootXSDs/Base.xsd"/>
	
    <xs:element name="submitQueryResponse" type="tns:submitQueryResponseType" />
    
	<xs:complexType name="submitQueryResponseType">
		<xs:sequence>
            <xs:element name="mcolCustomerId" type="base:mcolCustomerIdType" />
            <xs:element name="resultCount" type="xs:nonNegativeInteger" />
            <xs:element name="mcolResults" type="tns:mcolResultsType" />
			<xs:element name="status" type="base:statusType" />
		</xs:sequence>
	</xs:complexType>
	
    <xs:complexType name="mcolResultsType">
        <xs:choice>
            <xs:any minOccurs="0" maxOccurs="unbounded"/>
        </xs:choice>
    </xs:complexType>
    
</xs:schema>

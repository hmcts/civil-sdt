				<xs:element ref="tns:payee_address_line_4" minOccurs="0"/>
				<xs:element ref="tns:payee_address_line_5" minOccurs="0"/>
				<xs:element ref="tns:payee_postcode"/>
				<xs:element ref="tns:payee_telephone" minOccurs="0"/>

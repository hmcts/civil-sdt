			<xs:element name="bulkFeedbackRequest" type="tns:bulkFeedbackRequestType" />

		</xs:schema>

		<!-- BulkFeedbackResponse.xsd -->
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
            xmlns:tns="http://ws.sdt.moj.gov.uk/2013/sdt/BulkFeedbackResponseSchema" 
            targetNamespace="http://ws.sdt.moj.gov.uk/2013/sdt/BulkFeedbackResponseSchema" 
            xmlns:base="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" 
            xmlns:mresp="http://ws.sdt.moj.gov.uk/2013/mcol/ResponseDetailSchema" 
            elementFormDefault="qualified"
            version="1.0">
            
            <xs:import namespace="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema"/>
            <xs:import namespace="http://ws.sdt.moj.gov.uk/2013/mcol/ResponseDetailSchema"/>
            
            <xs:complexType name="bulkRequestStatusType">
                <xs:complexContent>
                    <xs:extension base="base:abstractResponseType">
                        <xs:sequence>
                            <xs:element name="customerReference" type="base:customerReferenceType" minOccurs="0"/>
                            <xs:element name="sdtBulkReference" type="base:sdtBulkReferenceType"/>
                            <xs:element name="submittedDate" type="xs:dateTime"  minOccurs="0"/>
                            <xs:element name="sdtService" type="xs:string"/>
                            <xs:element name="requestCount" type="xs:long"  minOccurs="0"/>
                            <xs:element name="bulkStatus" type="base:bulkStatusType"  minOccurs="0"/>
                        </xs:sequence>
                    </xs:extension>
                </xs:complexContent>
            </xs:complexType>
            
            <xs:complexType name="responseType">
                <xs:sequence>
                    <xs:element name="responseDetail">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element name="mcolResponseDetail" type="mresp:responseDetailType" minOccurs="0"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="status" type="base:individualStatusType"/>
                </xs:sequence>
                <xs:attribute name="requestType" type="base:requestTypeType" use="required"/>
                <xs:attribute name="requestId" type="base:requestIdType" use="required"/>
            </xs:complexType>
        
            <xs:complexType name="responsesType">
                <xs:sequence>
                    <xs:element name="response" type="tns:responseType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:complexType>
            
            <xs:complexType name="bulkFeedbackResponseType">
                <xs:sequence>
                    <xs:element name="bulkRequestStatus" type="tns:bulkRequestStatusType"/>
                    <xs:element name="responses" type="tns:responsesType" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>
        
            <xs:element name="bulkFeedbackResponse" type="tns:bulkFeedbackResponseType"/>
        
        </xs:schema>        
        <!-- SubmitQueryRequest.xsd -->
		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:tns="http://ws.sdt.moj.gov.uk/2013/sdt/SubmitQueryRequestSchema" 
			targetNamespace="http://ws.sdt.moj.gov.uk/2013/sdt/SubmitQueryRequestSchema"
			xmlns:base="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" 
			xmlns:mqry="http://ws.sdt.moj.gov.uk/2013/mcol/QuerySchema" 
			elementFormDefault="qualified"
			version="1.0">
	
	<xs:import namespace="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema"/>
    <xs:import namespace="http://ws.sdt.moj.gov.uk/2013/mcol/QuerySchema"/>
	
	<xs:complexType name="headerType">
		<xs:sequence>
			<xs:element name="sdtCustomerId" type="base:sdtCustomerIdType" />
			<xs:element name="targetApplicationId" type="base:targetApplicationIdType" />
            <xs:element name="queryReference" minOccurs="0"> 
                <xs:simpleType>
                    <xs:restriction base="base:sdtStringType">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
		</xs:sequence>
	</xs:complexType>
    
    <xs:complexType name="criterionType">
        <xs:choice>
            <xs:element name="mcolDefenceCriteria" type="mqry:mcolDefenceCriteriaType"/>
        </xs:choice>
        <xs:attribute name="criteriaType" type="base:criteriaTypeType" use="required"/>
    </xs:complexType>

	<xs:complexType name="criteriaType">
        <xs:sequence>
            <xs:element name="criterion" type="tns:criterionType" />
        </xs:sequence>
	</xs:complexType>
    
    <xs:complexType name="submitQueryRequestType">
        <xs:sequence>
            <xs:element name="header" type="tns:headerType" />
            <xs:element name="criteria" type="tns:criteriaType" />
        </xs:sequence>
    </xs:complexType>
    
    <xs:element name="submitQueryRequest" type="tns:submitQueryRequestType" />
    
</xs:schema>
        <!-- SubmitQueryResponse.xsd -->
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
            xmlns:tns="http://ws.sdt.moj.gov.uk/2013/sdt/SubmitQueryResponseSchema" 
            targetNamespace="http://ws.sdt.moj.gov.uk/2013/sdt/SubmitQueryResponseSchema"
            xmlns:base="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" 
            xmlns:mqry="http://ws.sdt.moj.gov.uk/2013/mcol/QuerySchema" 
            elementFormDefault="qualified"
            version="1.0">
            
            <xs:import namespace="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema"/>
            <xs:import namespace="http://ws.sdt.moj.gov.uk/2013/mcol/QuerySchema"/>
            
            <xs:complexType name="resultsType">
                <xs:choice>
                    <xs:element name="mcolDefenceDetail" type="mqry:mcolDefenceDetailType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
            </xs:complexType>
            
            <xs:complexType name="submitQueryResponseType">
                <xs:complexContent>
                    <xs:extension base="base:abstractResponseType">
                        <xs:sequence>
                            <xs:element name="sdtCustomerId" type="base:sdtCustomerIdType" />
                            <xs:element name="sdtService" type="xs:string"/>
                            <xs:element name="resultCount" type="xs:nonNegativeInteger" />
                            <xs:element name="results" type="tns:resultsType" />
                        </xs:sequence>
                    </xs:extension>
                </xs:complexContent>
            </xs:complexType>
            
            <xs:element name="submitQueryResponse" type="tns:submitQueryResponseType" />
            
        </xs:schema>
        
	</types>

	<message name="submitBulkRequest">
		<part name="bulkRequest" element="breq:bulkRequest" />
	</message>
	<message name="submitBulkResponse">
		<part name="bulkResponse" element="bresp:bulkResponse" />
	</message>
	<message name="bulkFeedbackRequest">
		<part name="bulkFeedbackRequest" element="bfreq:bulkFeedbackRequest" />
	</message>
	<message name="bulkFeedbackResponse">
		<part name="bulkFeedbackResponse" element="bfresp:bulkFeedbackResponse" />
	</message>
    <message name="submitQueryRequest">
        <part name="submitQueryRequest" element="qreq:submitQueryRequest" />
    </message>
    <message name="submitQueryResponse">
        <part name="submitQueryResponse" element="qresp:submitQueryResponse" />
    </message>

	<portType name="ISdtEndpointPortType">
		<operation name="submitBulk">
			<input message="tns:submitBulkRequest" />
			<output message="tns:submitBulkResponse" />
		</operation>
		<operation name="getBulkFeedback">
			<input message="tns:bulkFeedbackRequest" />
			<output message="tns:bulkFeedbackResponse" />
		</operation>
        <operation name="submitQuery">
            <input message="tns:submitQueryRequest" />
            <output message="tns:submitQueryResponse" />
        </operation>
	</portType>

	<binding name="SdtEndpointHttpPortBinding" type="tns:ISdtEndpointPortType">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<operation name="submitBulk">
			<soap:operation soapAction="" />
			<input>
				<soap:body use="literal" />
			</input>
			<output>
				<soap:body use="literal" />
			</output>
		</operation>
		<operation name="getBulkFeedback">
			<soap:operation soapAction="" />
			<input>
				<soap:body use="literal" />
			</input>
			<output>
				<soap:body use="literal" />
			</output>
		</operation>
		<operation name="submitQuery">
			<soap:operation soapAction="" />
			<input>
				<soap:body use="literal" />
			</input>
			<output>
				<soap:body use="literal" />
			</output>
		</operation>
	</binding>

	<service name="SdtEndpoint">
		<port name="SdtEndpointPort" binding="tns:SdtEndpointHttpPortBinding">
			<soap:address location="http://ws.sdt.moj.gov.uk/sdt/service" />
		</port>
	</service>


<?xml version="1.0" encoding="utf-8"?>
<definitions name="SdtEndpoint" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.xmlsoap.org/wsdl/"
	xmlns:tns="http://ws.sdt.moj.gov.uk/2013/sdt/SdtEndpoint" xmlns:breq="http://ws.sdt.moj.gov.uk/2013/sdt/BulkRequestSchema"
	xmlns:bresp="http://ws.sdt.moj.gov.uk/2013/sdt/BulkResponseSchema"
	xmlns:bfreq="http://ws.sdt.moj.gov.uk/2013/sdt/BulkFeedbackRequestSchema"
	xmlns:bfresp="http://ws.sdt.moj.gov.uk/2013/sdt/BulkFeedbackResponseSchema"
    xmlns:qreq="http://ws.sdt.moj.gov.uk/2013/sdt/SubmitQueryRequestSchema"
    xmlns:qresp="http://ws.sdt.moj.gov.uk/2013/sdt/SubmitQueryResponseSchema"
    xmlns:sas="http://ws.sdt.moj.gov.uk/2013/mcol/SetAsideSchema"
    targetNamespace="http://ws.sdt.moj.gov.uk/2013/sdt/SdtEndpoint">

	<types>

		<!-- Base.xsd -->
		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:tns="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema"
			targetNamespace="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema"
			elementFormDefault="qualified" version="1.0">

			<xs:simpleType name="sdtStringType">
				<xs:restriction base="xs:string">
					<xs:pattern value="[a-zA-Z0-9\-\+\.,Â£@\?'\(\)/%_ &amp;!:]*" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="sdtNonBlankStringType">
				<xs:restriction base="tns:sdtStringType">
					<xs:pattern value="(\s*[^\s]\s*)+"/>
				</xs:restriction>
			</xs:simpleType>


			<xs:simpleType name="sdtCustomerIdType">
				<xs:restriction base="xs:long">
					<xs:pattern value="[1-9][0-9]{7}" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="targetApplicationIdType">
				<xs:restriction base="tns:sdtStringType">
					<xs:length value="4" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="requestTypeType">
				<xs:restriction base="tns:sdtStringType">
					<xs:maxLength value="50" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="criteriaTypeType">
				<xs:restriction base="tns:sdtStringType">
					<xs:maxLength value="50" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="sdtBulkReferenceType">
				<xs:restriction base="xs:string">
					<xs:length value="29" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="customerReferenceType">
				<xs:restriction base="xs:string">
					<xs:minLength value="1" />
					<xs:maxLength value="32" />
					<xs:pattern value="[0-9a-zA-Z_\-]+"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="claimNumberType">
				<xs:restriction base="xs:string">
					<xs:pattern value="[0-9A-Za-z]{8}" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="warrantNumberType">
				<xs:restriction base="xs:string">
					<xs:pattern value="[0-9A-Za-z]{8}" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="defendantIdType">
				<xs:restriction base="xs:string">
					<xs:pattern value="1|2" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="lineType">
				<xs:restriction base="tns:sdtStringType">
					<xs:minLength value="1" />
					<xs:maxLength value="30" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="nonBlankLineType">
				<xs:restriction base="tns:sdtNonBlankStringType">
					<xs:minLength value="1" />
					<xs:maxLength value="30" />
				</xs:restriction>
			</xs:simpleType>
			
			<xs:complexType name="sotSignatureType">
				<xs:sequence>
					<xs:element name="flag">
						<xs:simpleType>
							<xs:restriction base="xs:boolean">
								<xs:pattern value="true" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="name">
						<xs:simpleType>
							<xs:restriction base="tns:sdtNonBlankStringType">
								<xs:minLength value="1" />
								<xs:maxLength value="30" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:simpleType name="courtCodeType">
				<xs:restriction base="tns:sdtNonBlankStringType">
					<xs:minLength value="1" />
					<xs:maxLength value="3" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="courtNameType">
				<xs:restriction base="tns:sdtNonBlankStringType">
					<xs:minLength value="1" />
					<xs:maxLength value="40" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="postcodeType">
				<xs:restriction base="xs:string">
					<xs:pattern value="[A-Z]{1,2}[0-9R][0-9A-Z]? [0-9][A-Z-[CIKMOV]]{2}" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="nonUKPostcodeType">
				<xs:restriction base="xs:string">
					<xs:maxLength value="8" />
					<xs:pattern value="[0-9a-zA-Z ]+"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="addressType">
				<xs:sequence>
					<xs:element name="line1" type="tns:nonBlankLineType" />
					<xs:element name="line2" type="tns:nonBlankLineType" />
					<xs:element name="line3" type="tns:lineType" minOccurs="0" />
					<xs:element name="line4" type="tns:lineType" minOccurs="0" />
					<xs:element name="postcode" type="tns:postcodeType" />
				</xs:sequence>
			</xs:complexType>

			<xs:simpleType name="requestIdType">
				<xs:restriction base="xs:string">
					<xs:minLength value="1" />
					<xs:maxLength value="24" />
					<xs:pattern value="[0-9a-zA-Z_\-]+"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="sdtRequestIdType">
				<xs:restriction base="tns:sdtNonBlankStringType">
					<xs:minLength value="1" />
					<xs:maxLength value="40" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="statusCodeType">
				<xs:restriction base="xs:string">
					<xs:enumeration value="Ok" />
					<xs:enumeration value="Error" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="createStatusCodeType">
				<xs:restriction base="xs:string">
					<xs:enumeration value="Accepted" />
					<xs:enumeration value="Awaiting Data" />
					<xs:enumeration value="Initially Accepted" />
					<xs:enumeration value="Rejected" />
					<xs:enumeration value="Error" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="updateStatusCodeType">
				<xs:restriction base="xs:string">
					<xs:enumeration value="Accepted" />
					<xs:enumeration value="Rejected" />
					<xs:enumeration value="Resubmit Message" />
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="errorType">
				<xs:sequence>
					<xs:element name="code">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="1" />
								<xs:maxLength value="32" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="description">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="1" />
								<xs:maxLength value="255" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="statusType">
				<xs:sequence>
					<xs:element name="error" type="tns:errorType" minOccurs="0" />
				</xs:sequence>
				<xs:attribute name="code" type="tns:statusCodeType" use="required"/>
			</xs:complexType>

			<xs:complexType name="createStatusType">
				<xs:sequence>
					<xs:element name="error" type="tns:errorType" minOccurs="0" />
				</xs:sequence>
				<xs:attribute name="code" type="tns:createStatusCodeType" use="required"/>
			</xs:complexType>

			<xs:complexType name="updateStatusType">
				<xs:sequence>
					<xs:element name="error" type="tns:errorType" minOccurs="0" />
				</xs:sequence>
				<xs:attribute name="code" type="tns:updateStatusCodeType" use="required"/>
			</xs:complexType>

			<xs:simpleType name="bulkStatusCodeType">
				<xs:restriction base="xs:string">
					<xs:enumeration value="Validated" />
					<xs:enumeration value="Completed" />
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="individualStatusCodeType">
				<xs:restriction base="xs:string">
					<xs:enumeration value="Received" />
					<xs:enumeration value="Forwarded" />
					<xs:enumeration value="Initially Accepted" />   
					<xs:enumeration value="Awaiting Data" />
					<xs:enumeration value="Accepted" />
					<xs:enumeration value="Rejected" />
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="bulkStatusType">
				<xs:sequence>
					<xs:element name="error" type="tns:errorType" minOccurs="0" />
				</xs:sequence>
				<xs:attribute name="code" type="tns:bulkStatusCodeType" use="required"/>
			</xs:complexType>

			<xs:complexType name="individualStatusType">
				<xs:sequence>
					<xs:element name="error" type="tns:errorType" minOccurs="0" />
				</xs:sequence>
				<xs:attribute name="code" type="tns:individualStatusCodeType" use="required"/>
			</xs:complexType>

			<xs:complexType name="abstractResponseType" abstract="true">
				<xs:sequence>
					<xs:element name="status" type="tns:statusType"/>
				</xs:sequence>
			</xs:complexType>

            <xs:simpleType name="defenceType">
                <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="8000" />
                </xs:restriction>
            </xs:simpleType>

		</xs:schema>     
		<!-- Claim.xsd -->
		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
			targetNamespace="http://ws.sdt.moj.gov.uk/2013/mcol/ClaimSchema"
			xmlns:tns="http://ws.sdt.moj.gov.uk/2013/mcol/ClaimSchema" 
			xmlns:base="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" 
			elementFormDefault="qualified"
			version="1.0">

			<xs:import namespace="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema"/>
			
			<xs:complexType name="claimantType">
				<xs:sequence>
					<xs:element name="name">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1" />
								<xs:maxLength value="60" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="address">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="line1" type="base:nonBlankLineType" />
								<xs:element name="line2" type="base:nonBlankLineType" />
								<xs:element name="line3" type="base:lineType" minOccurs="0" />
								<xs:element name="line4" type="base:lineType" minOccurs="0" />
								<xs:element name="postcode" type="base:nonUKPostcodeType" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			
			<xs:simpleType name="particularsType">
				<xs:restriction base="base:sdtStringType">
					<xs:minLength value="1"/>
					<xs:maxLength value="45"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="interestType">
				<xs:sequence>
					<xs:element name="dailyAmount" type="xs:unsignedLong"/>
					<xs:element name="owedDate" type="xs:date"/>
					<xs:element name="claimDate" type="xs:date"/>
					<xs:element name="claimAmountInterestBase" type="xs:unsignedLong"/>
				</xs:sequence>
			</xs:complexType>
			
			<xs:simpleType name="claimantReferenceType">
				<xs:restriction base="base:sdtNonBlankStringType">
					<xs:minLength value="1" />
					<xs:maxLength value="24" />
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="correspondenceDetailType">
				<xs:sequence>
					<xs:element name="name">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1" />
								<xs:maxLength value="30" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="address"> 
						<xs:complexType>
							<xs:sequence>
								<xs:element name="line1" type="base:nonBlankLineType" />
								<xs:element name="line2" type="base:nonBlankLineType" />
								<xs:element name="postcode" type="base:nonUKPostcodeType" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element> 
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="defendantType">
				<xs:sequence>
					<xs:element name="name">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1" />
								<xs:maxLength value="60" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="address" type="base:addressType" />
				</xs:sequence>
			</xs:complexType>
			
			<xs:complexType name="claimType">
				<xs:sequence>
					<xs:element name="claimantReference" type="tns:claimantReferenceType" minOccurs="0"/>
					<xs:element name="claimant" type="tns:claimantType" minOccurs="0"/>
					<xs:element name="claimantCorrespondence" type="tns:correspondenceDetailType" minOccurs="0"/>
					<xs:element name="defendant1" type="tns:defendantType"/>
					<xs:element name="defendant2" type="tns:defendantType" minOccurs="0"/>
					<xs:element name="sendParticularsSeparately" type="xs:boolean"/>
					<xs:element name="reserveRightToClaimInterest" type="xs:boolean"/>
					<xs:element name="interest" type="tns:interestType" minOccurs="0"/>
					<xs:element name="claimAmount" type="xs:unsignedLong"/>
					<xs:element name="solicitorCost" type="xs:unsignedLong" minOccurs="0" />
					<xs:element name="particulars" type="tns:particularsType" minOccurs="1" maxOccurs="24"/>
					<xs:element name="sotSignature" type="base:sotSignatureType"/>
				</xs:sequence>
			</xs:complexType>
			
			<xs:element name="mcolClaim" type="tns:claimType"/>

		</xs:schema>


		<!-- Warrant.xsd -->
		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:tns="http://ws.sdt.moj.gov.uk/2013/mcol/WarrantSchema" 
			targetNamespace="http://ws.sdt.moj.gov.uk/2013/mcol/WarrantSchema"
			xmlns:base="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" 
			elementFormDefault="qualified"
			version="1.0">
			
			<xs:import namespace="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" />
			
			<xs:simpleType name="additionalNotesType">
				<xs:restriction base="base:sdtNonBlankStringType">
					<xs:minLength value="1" />
					<xs:maxLength value="120" />
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="warrantType">
				<xs:sequence>
					<xs:element name="claimNumber" type="base:claimNumberType" />
					<xs:element name="defendantId" type="base:defendantIdType" />
					<xs:element name="defendantAddress" type="base:addressType" minOccurs="0" />
					<xs:element name="balanceOfDebt" type="xs:unsignedLong" />
					<xs:element name="warrantAmount" type="xs:unsignedLong" />
					<xs:element name="solicitorCost" type="xs:unsignedLong" minOccurs="0" />
					<xs:element name="additionalNotes" type="tns:additionalNotesType" minOccurs="0" />
					<xs:element name="sotSignature" type="base:sotSignatureType"/>
				</xs:sequence>
			</xs:complexType>

			<xs:element name="mcolWarrant" type="tns:warrantType" />

		</xs:schema>

		<!-- Judgment.xsd -->
		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
			targetNamespace="http://ws.sdt.moj.gov.uk/2013/mcol/JudgmentSchema"
			xmlns:tns="http://ws.sdt.moj.gov.uk/2013/mcol/JudgmentSchema" 
			xmlns:base="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema" 
			xmlns:wnt="http://ws.sdt.moj.gov.uk/2013/mcol/WarrantSchema" 
			elementFormDefault="qualified"
			version="1.0">

			<xs:import namespace="http://ws.sdt.moj.gov.uk/2013/sdt/BaseSchema"/>
			<xs:import namespace="http://ws.sdt.moj.gov.uk/2013/mcol/WarrantSchema"/>
			
			<xs:simpleType name="judgmentTypeType">
				<xs:restriction base="xs:string">
					<xs:enumeration value="A"/>
					<xs:enumeration value="D"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:simpleType name="instalmentFrequencyType">
				<xs:restriction base="xs:string">
					<xs:enumeration value="W"/>
					<xs:enumeration value="F"/>
					<xs:enumeration value="M"/>
				</xs:restriction>
			</xs:simpleType>

			<xs:complexType name="instalmentPaymentType">
				<xs:sequence>
					<xs:element name="amount" type="xs:unsignedLong"/>
					<xs:element name="frequency" type="tns:instalmentFrequencyType"></xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="fullPaymentType">
				<xs:sequence>
					<xs:element name="fullByDate" type="xs:date"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="immediatePaymentType"/>

			<xs:complexType name="paymentScheduleType">
				<xs:choice>
					<xs:element name="inFullByPayment" type="tns:fullPaymentType"/>
					<xs:element name="instalment" type="tns:instalmentPaymentType"/>
					<xs:element name="immediatePayment" type="tns:immediatePaymentType"/>
				</xs:choice>
			</xs:complexType>
		 
			<xs:simpleType name="payeeLineType">
				<xs:restriction base="base:sdtNonBlankStringType">
					<xs:minLength value="1" />
					<xs:maxLength value="35" />
				</xs:restriction>
			</xs:simpleType>

		    <xs:simpleType name="payeeNonBlankLineType">
		        <xs:restriction base="base:sdtNonBlankStringType">
		            <xs:minLength value="1" />
		            <xs:maxLength value="35" />
		        </xs:restriction>
		    </xs:simpleType>

			<xs:complexType name="payeeAddressType">
				<xs:sequence>
					<xs:element name="line1" type="tns:payeeNonBlankLineType" />
					<xs:element name="line2" type="tns:payeeNonBlankLineType" />
					<xs:element name="line3" type="tns:payeeLineType" minOccurs="0" />
					<xs:element name="line4" type="tns:payeeLineType" minOccurs="0" />
					<xs:element name="line5" type="tns:payeeLineType" minOccurs="0" />
					<xs:element name="postcode" type="base:postcodeType" minOccurs="0" />
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="payeeType">
				<xs:sequence>
					<xs:element name="name">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>
								<xs:maxLength value="70"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="address" type="tns:payeeAddressType"/>
					<xs:element name="telephoneNumber" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="[0-9][\d-]{0,13}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="dxNumber" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>
								<xs:maxLength value="35"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="faxNumber" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>
								<xs:maxLength value="24"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="email" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>
								<xs:maxLength value="254"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="pcm" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>
								<xs:maxLength value="2"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="reference" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>
								<xs:maxLength value="24"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="bankAccountNumber" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>
								<xs:maxLength value="8"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="bankAccountHolder" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>
								<xs:maxLength value="70"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="bankSortCode" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="base:sdtNonBlankStringType">
								<xs:minLength value="1"/>

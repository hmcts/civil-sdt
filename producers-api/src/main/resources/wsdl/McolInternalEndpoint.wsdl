				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="defenceDetailsType">
				<xs:sequence>
					<xs:element name="defenceDetail" type="tns:defenceDetailType"
						maxOccurs="unbounded" />
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="defenceDetailType">
				<xs:sequence>
					<xs:element name="claimNumber" type="base:documentNumberType" />
					<xs:element name="defendants">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="defendant" type="tns:defendantType"
									maxOccurs="2" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="defendantType">
				<xs:sequence>
					<xs:element name="filedDate" type="xs:dateTime" />
					<xs:element name="responseType" type="tns:responseType" />
				</xs:sequence>
				<xs:attribute name="defendantId" type="base:defendantIdType" />
			</xs:complexType>

			<!-- Response Type is DE, DC or PA -->
			<xs:simpleType name="responseType">
				<xs:restriction base="xs:string">
					<xs:enumeration value="DE" />
					<xs:enumeration value="DC" />
					<xs:enumeration value="PA" />
				</xs:restriction>
			</xs:simpleType>
		</xs:schema>

